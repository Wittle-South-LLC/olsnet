{
  "name": "olsnet",
  "version": "0.2.0",
  "description": "Client web application for Our Life Stories.net",
  "main": "index.js",
  "author": "Eric L. Wittle",
  "license": "ISC",
  "scripts": {
    "dev": "webpack -d --devtool='source-map' --watch",
    "build": "webpack -p --aiv-log-none",
    "test": "MESSAGE_LEVEL='debug' mocha --require ignore-styles --compilers js:babel-register",
    "mocha": "mocha --require ignore-styles --compilers js:babel-register",
    "cover": "nyc _mocha --require ignore-styles --compilers js:babel-register ",
    "build:langs": "babel scripts/translate.js | node"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "**/test-*.js",
      "**/TestUtils.js"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "immutable": "^3.8.1",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-router": "^4.1.1",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "chai": "^4.0.0",
    "cheerio": "^0.22.0",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "css-loader": "^0.28.3",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^1.20.2",
    "file-loader": "^0.11.1",
    "html-webpack-inline-chunk-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "immutablediff": "^0.4.4",
    "intl-messageformat-parser": "^1.3.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "nyc": "^10.3.2",
    "redux-mock-store": "^1.2.3",
    "sinon": "^2.3.2",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack-auto-inject-version": "^0.5.14",
    "webpack-chunk-hash": "^0.4.0"
  }
}
